@startuml
!include style.puml
actor User
participant Astra as ":Astra"
participant Parser as ":Parser"
participant C as ":ListGpaCommand"
participant NB as ":Notebook"
participant GL as ":GpaList"
participant UI as ":Ui"

User -> Astra:"list gpa"
activate Astra
Astra -> Parser: parse("list gpa")
activate Parser
create C
Parser -> C: new ListGpaCommand()
activate C
return
return

Astra -> C: execute(AL, UI, NB)
activate C

C -> NB: getGpaList()
activate NB
NB -> GL : toList()
activate GL
return List<GpaList>
return GpaList

alt list is empty
	C -> UI: showMessage("No GPA entries yet.")
	activate UI
	return
	C --> Astra: false
else list has items
	C -> UI: showMessage(" 1. <entry>\n 2. <entry> ...")
	activate UI
	return
end
C --> Astra:false
deactivate C
Astra --> User
destroy C
@enduml